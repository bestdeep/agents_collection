{
  "description": "Example configuration for BabyAI Agent",
  "agent": {
    "api_key": "your-api-key-here",
    "base_url": "https://api.openai.com/v1",
    "model": "gpt-3.5-turbo",
    "max_tokens": 512,
    "temperature": 0.7,
    "top_p": 1.0,
    "timeout": 60.0,
    "max_rounds": 20,
    "max_retries": 3,
    "retry_delay": 1.0
  },
  "environment": {
    "env_server": "http://localhost:36001",
    "data_len": 200,
    "timeout": 300
  },
  "evaluation": {
    "num_tasks": 10,
    "start_idx": 0,
    "output_dir": "./babyai_results",
    "save_conversations": true
  }
}
